    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.RegistActivity" />
        <activity android:name=".Activity.CheckActivity" />
        <activity android:name=".Activity.CodeActivity" />
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.ContainerActivity" />
        <activity android:name=".Activity.LessonMeActivity" />
        <activity android:name=".Activity.JoinActivity" />
        <activity android:name=".Activity.ChosenCourseActivity" />
        <activity android:name=".Activity.RecordMeActivity" />
        <activity android:name=".Activity.EachDetailActivity"></activity>
    </application>

</manifest>